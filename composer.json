{
    "name": "osmphp/framework-project",
    "description": "A project template based on osmphp/framework package",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "osmphp/framework": "^0.8",
        "nesbot/carbon": "^2.48",
        "michelf/php-markdown": "^1.9",
        "nikic/fast-route": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "My\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "My\\Tests\\": "tests/",
            "My\\Samples\\": "samples/"
        }
    }
}
