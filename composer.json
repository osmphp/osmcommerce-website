{
    "name": "osmphp/osmsoftware-website",
    "description": "A project template based on osmphp/framework package",
    "license": "AGPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "osmphp/framework": "^0.14",
        "nesbot/carbon": "^2.48",
        "michelf/php-markdown": "^1.9",
        "symfony/dom-crawler": "^5",
        "symfony/css-selector": "^5",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "My\\": "src/",
            "Osm\\Framework\\": "packages/framework/",
            "Osm\\Data\\": "packages/data/",
            "Osm\\Blog\\": "packages/blog/",
            "Osm\\Docs\\": "packages/docs/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "My\\Tests\\": "tests/",
            "My\\Samples\\": "samples/"
        }
    }
}
